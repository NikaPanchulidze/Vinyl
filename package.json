{
    "name": "backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=development nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test tsx --test 'src/**/*.spec.ts'",
        "test:watch": "cross-env NODE_ENV=test tsx --test --watch 'src/**/*.spec.ts'",
        "test:debug": "cross-env NODE_ENV=test node --inspect-brk `which tsx` --test 'src/**/*.spec.ts'",
        "test:cov": "cross-env NODE_ENV=test c8 --reporter=lcov npm run test",
        "test:e2e": "cross-env NODE_ENV=e2e tsx --test test/**/*.e2e-spec.ts",
        "test:e2e:watch": "cross-env NODE_ENV=e2e tsx --test --watch test/**/*.e2e-spec.ts",
        "seed:discogs": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/scripts/discogs.seed.ts",
        "migration:generate": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/ormconfig.ts",
        "migration:run": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/ormconfig.ts",
        "migration:revert": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/ormconfig.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.913.0",
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/jwt": "^11.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.2.1",
        "@nestjs/typeorm": "^11.0.0",
        "@types/qs": "^6.14.0",
        "axios": "^1.12.2",
        "bcrypt": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "cookie-parser": "^1.4.7",
        "cross-env": "^10.1.0",
        "dotenv": "^17.2.3",
        "multer": "^2.0.2",
        "multer-s3": "^3.0.1",
        "nodemailer": "^7.0.9",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.16.3",
        "raw-body": "^3.0.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "stripe": "^19.1.0",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.27"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/express": "^5.0.3",
        "@types/multer": "^2.0.0",
        "@types/node": "^22.18.12",
        "@types/nodemailer": "^7.0.2",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/sinon": "^17.0.4",
        "@types/supertest": "^6.0.3",
        "c8": "^10.1.3",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "prettier": "^3.4.2",
        "sinon": "^21.0.0",
        "source-map-support": "^0.5.21",
        "sqlite3": "^5.1.7",
        "supertest": "^7.1.4",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.6",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    }
}
